{
	"name": "website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev:pre": "pnpm run codegen",
		"dev": "pnpm run dev:pre && vite dev",
		"build": "pnpm run codegen && NODE_ENV=production vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"test:unit": "vitest",
		"update": "pnpm update",
		"lint": "pnpm run dev:pre && prettier --check \"**/*\" -u && eslint . --ext .js,.ts,.svelte && pnpm run svelte:check",
		"svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json | tee /dev/null",
		"format": "prettier --write \"**/*\" -u",
		"clean": "rm -rf build .svelte-kit src/gql",
		"codegen": "graphql-codegen --config codegen.cjs"
	},
	"devDependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.4.2",
		"@fortawesome/free-regular-svg-icons": "^6.4.2",
		"@fortawesome/free-solid-svg-icons": "^6.4.2",
		"@graphql-codegen/cli": "4.0.1",
		"@graphql-codegen/client-preset": "^4.1.0",
		"@graphql-codegen/introspection": "^4.0.0",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-document-nodes": "^4.0.1",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@playwright/test": "^1.38.1",
		"@rollup/plugin-commonjs": "^25.0.4",
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.2.1",
		"@rollup/plugin-replace": "^5.0.2",
		"@scuffle/player": "workspace:*",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.25.1",
		"@types/fs-extra": "^11.0.2",
		"@types/node": "^20.8.2",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"concurrently": "^8.2.1",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.34.0",
		"espree": "^9.6.1",
		"fast-glob": "^3.3.1",
		"fs-extra": "^11.1.1",
		"moment": "^2.29.4",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"rollup": "^3.29.4",
		"sass": "^1.68.0",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-fa": "^3.0.4",
		"svelte-turnstile": "^0.5.0",
		"svelte2tsx": "^0.6.22",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"ulid": "^2.3.0",
		"vite": "^4.4.10",
		"vitest": "^0.33.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/be-vietnam-pro": "^5.0.14",
		"@fontsource/comfortaa": "^5.0.14",
		"@urql/exchange-auth": "^2.1.6",
		"@urql/svelte": "^4.0.4",
		"graphql": "^16.8.1",
		"graphql-ws": "^5.14.1",
		"urql": "^4.0.5",
		"wonka": "^6.3.4",
		"zod": "^3.22.4"
	}
}
